{
  "name": "autocherry",
  "displayName": "AutoCherry",
  "publisher": "jayveer-sodha",
  "description": "AutoCherry lets you quickly cherry-pick one or more commits across branches without touching the command line. Simple, fast, and built right into VS Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onUri"
  ],
  "main": "extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autocherry.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
