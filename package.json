{
  "name": "cherrypicker",
  "displayName": "CherryPicker",
  "publisher": "jayveersodha",
  "description": "CherryPicker lets you quickly cherry-pick one or more commits across branches without touching the command line. Simple, fast, and built right into VS Code.",
  "icon": "media/cherry.png",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onUri"
  ],
  "main": "extension/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cherrypicker",
          "title": "CherryPicker",
          "icon": "media/cherry.png"
        }
      ]
    },
    "views": {
      "cherrypicker": [
        {
          "type": "webview",
          "id": "cherrypickerView",
          "name": " ðŸš€ Smart Cherry-Picker",
          "icon": "media/cherry.png"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "build": "vite build --config webview/vite.config.js",
    "dev": "nodemon --watch webview --ext js,jsx,ts,tsx,css --ignore webview/dist --exec \"npm run build\""
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "nodemon": "^3.1.10",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "node-fetch": "^3.3.2",
    "simple-git": "^3.28.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jayveer-Sodha/AutoCherry.git"
  },
  "license": "SEE LICENSE",
  "author": "Jayveer Sodha"
}
